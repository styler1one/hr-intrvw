{
  "rewrites": [
    { "source": "/api/templates", "destination": "/api/templates" },
    { "source": "/api/start", "destination": "/api/start" },
    { "source": "/api/chat", "destination": "/api/chat" },
    { "source": "/api/suggestions", "destination": "/api/suggestions" },
    { "source": "/api/export", "destination": "/api/export" },
    { "source": "/api/generate_report", "destination": "/api/generate_report" },
    { "source": "/api/generate_checklist", "destination": "/api/generate_checklist" },
    { "source": "/api/test", "destination": "/api/test" }
  ],
  "functions": {
    "api/chat.py": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/suggestions.py": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/*.py": {
      "maxDuration": 30,
      "memory": 512
    }
  }
}
